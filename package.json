{
  "name": "rengo-ui-kit",
  "version": "1.0.0",
  "description": "A uikit used on https://casperswap.xyz",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "unpkg": "lib/index.umd.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "concurrently \" npm run build:compile \" \" npm run watch \" \" npm run start --prefix example \" ",
    "prettier": "prettier --write .",
    "test:lint": "eslint src/components/**/*.{js,ts,tsx}",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom --coverage --collectCoverageFrom=src/components/**/*.{js,ts,tsx}",
    "test:watch": "react-scripts test --env=jsdom --coverage --collectCoverageFrom=src/components/**/*.{js,ts,tsx}",
    "test:build": "npm run build",
    "test": "npm run test:unit && npm run test:lint && npm run test:build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build:compile": "rollup -c",
    "build": "npm run build:esm && npm run build:compile",
    "watch": "rollup -c --watch",
    "build:esm": "babel src  --extensions \".js,.ts,.tsx\" --out-dir build --copy-files --ignore __tests__,spec.js,test.js,stories.js,__snapshots__",
    "prepublishOnly": "yarn build",
    "clean": "rm -rf example/node_modules && rm -rf node_modules/",
    "install-ui-kit": "yarn install && yarn build:compile",
    "install-react-example": "cd example && yarn install",
    "setup": "yarn run clean && yarn run install-ui-kit && yarn run start"
  },
  "np": {
    "publish": false,
    "tests": false
  },
  "keywords": [
    "stroybook",
    "react",
    "typescript"
  ],
  "author": "Sebastian Barcelona",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/runtime": "^7.9.6",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-centered": "^5.3.18",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@svgr/rollup": "^7.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@types/react": "^18.0.27",
    "@types/styled-components": "^5.1.26",
    "babel-eslint": "^10.0.3",
    "babel-loader": "8.1.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-styled-components": "^1.10.7",
    "concurrently": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-ts-lambdas": "^1.2.0",
    "eslint-plugin-react": "^7.19.0",
    "postcss": "^8.4.21",
    "prettier": "^2.0.5",
    "prettier-eslint-cli": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^4.0.3",
    "rollup": "^2.34.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-uglify": "^6.0.4",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.8"
  },
  "dependencies": {
    "@rollup/plugin-url": "^8.0.1",
    "classnames": "^2.3.2",
    "react-feather": "^2.0.10",
    "react-styled-flexboxgrid": "^3.2.1",
    "recharts": "^2.5.0",
    "styled-components": "^5.3.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rengo-Labs/Rengo-UI-Kit"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
